{
	"schemaVersion": 1,
	"id": "newfab",
	"version": "${version}",
	"name": "NewFab",
	"description": "NewFab is a mod that adds a few custom items with custom held animation, and new features like a backslot for you to keep your equipment ready for any fight, trident returns to original slot and more...",
	"authors": [
		"ormanu"
	],
	"contact": {
		"homepage": "https://www.instagram.com/matei_orman/",
		"sources": "https://github.com/ormanu"
	},
	"license": "MIT",
	"icon": "assets/newfab/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"ormanu.newfab.NewFab"
		],
		"fabric-datagen": [
			"ormanu.newfab.NewFabDataGenerator"
		],
		"client": [
			"ormanu.newfab.NewFabClient"
		],
		"modmenu": [
			"ormanu.newfab.config.ModMenu"
		]
	},
	"mixins": [
		"newfab.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.10",
		"minecraft": "~1.21.4",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}